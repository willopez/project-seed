  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-eslint": "^8.0.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.3.0",
    "gulp": "^3.9.1",
    "gulp-csso": "^3.0.0",
    "gulp-sass": "^3.1.0",
    "postcss-cli": "^4.1.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "prettier": "^1.7.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "scripts": {
    "test": "",
    "gulp": "./node_modules/gulp/bin/gulp.js",
    "postcss": "postcss --config postcss.config.js --replace static/css/app.css",
    "css:dev": "node-sass --output-style expanded --precision 6 styles/app.scss static/css/app.css && npm run postcss",
    "css:prod": "npm run gulp && npm run postcss",
    "start": "npm run css:dev && NODE_ENV=development PORT=3000 nodemon -w index.js",
    "build": "next build",
    "production": "npm run css:prod && npm run build && node index.js"
  },
  "engines": {
    "node": "8.x.x"
  }
}
