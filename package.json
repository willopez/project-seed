    "next": "4.0.0-beta.2",
    "node-sass": "^4.5.3",
    "popper.js": "^1.12.3",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
